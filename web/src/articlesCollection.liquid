---
layout: postLayout
tags: 
 - articlesCollection
pagination: 
  data: article
  alias: article
  size: 1
  addAllPagesToCollections: true 
eleventyComputed: 
  metaTitle: "{{ article.title }}"
  metaDescription: "{{ article.leading }}"
  metaImageUrl: "{{ article.featuredImage | default: siteSettings.metaImageUrl }}"
  id: "{{ article._id }}"
permalink: "{{ article.fullSlug }}/"    
--- 
